{
	"compilerOptions": {
		"strict": true,
		"outDir": "./dist/",
		"noImplicitAny": true,
		"module": "es6",
		"target": "es5",
		"jsx": "react-jsx",
		"allowJs": true,
		"moduleResolution": "node",
		"plugins": [
			{
				"name": "typescript-styled-plugin",
				"tags": [
					"styled",
					"css",
					"sty"
				]
			}
		]
	},
	"include": ["**/*.ts", "**/*.tsx"]
}